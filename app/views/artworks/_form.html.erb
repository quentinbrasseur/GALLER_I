<%= simple_form_for @artwork do |f| %>
  <%= f.input :title %>
  <%= f.input :category %>
  <%= f.input :location %>
  <%= f.input :height, as: :integer %>
  <%= f.input :width, as: :integer  %>
  <%= f.input :depth, as: :integer  %>
  <%= f.input :weight, as: :integer  %>
  <%= f.input :photo, as: :attachinary %>
  <div class="text-center">
  <br>
  <%= f.button :submit, class: "btn btn-primary" %>
  <br>
  </div>
<% end %>
