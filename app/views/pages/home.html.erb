
<h1> HOME PAGE</h1>


<div class="search-bar">
    <form class="form-inline" action="/search" method="GET">
      <label class="sr-only" for="inlineFormInput">City</label>
      <input name="city" type="text" class="form-control mb-2 mr-sm-2 mb-sm-0" id="inlineFormInput" placeholder="Enter your city">


      <label class="sr-only" for="inlineFormInputGroup">When</label>
      <div class="input-group mb-2 mr-sm-2 mb-sm-0">
        <input name="date_range" type="text" class="form-control" id="dateRange" placeholder="When">
      </div>
      <input type="submit"  class="btn btn-primary" value="Search!">

    </form>
  </div>





<li><%= link_to "See Artworks", artworks_path %></li>
<li><%= link_to "See Venues", venues_path %></li>
<li><%= link_to "Create Venues", new_venue_path %></li>
<% if current_user %>

<h3>My Venues</h3>
  <ul>
  <% current_user.venues.each do |venue| %>
    <li><%= venue.title %></li>

  <% end %>
  </ul>
<% end %>
